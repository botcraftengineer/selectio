{
  "name": "@selectio/jobs",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "default": "./src/inngest/client.ts"
    },
    "./channels": {
      "default": "./src/inngest/channels-client.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bun with-env bun run --watch src/inngest/server.ts",
    "inngest": "npx inngest-cli@latest dev -u http://localhost:8000/api/inngest",
    "hh:parse": "bun with-env bun run src/hh-response-parser.ts",
    "hh:enrich": "bun with-env bun run src/hh-enrich-responses.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "bun run --env-file ../../.env"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "1.0.31",
    "@ai-sdk/openai": "2.0.77",
    "@inngest/realtime": "0.4.5",
    "@selectio/config": "workspace:*",
    "@selectio/db": "workspace:*",
    "@selectio/lib": "workspace:*",
    "@selectio/prompts": "workspace:*",
    "@selectio/telegram-bot": "workspace:*",
    "@selectio/tg-client": "workspace:*",
    "@t3-oss/env-core": "0.13.8",
    "ai": "5.0.106",
    "axios": "^1.13.2",
    "crawlee": "3.15.3",
    "inngest": "^3.46.0",
    "langfuse": "^3.38.6",
    "puppeteer": "24.32.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "string-strip-html": "13.5.0",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@selectio/tsconfig": "workspace:*",
    "@types/bun": "1.3.3",
    "@types/node": "24.10.1",
    "cross-env": "10.1.0",
    "inngest-cli": "^1.14.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
