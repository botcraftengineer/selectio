{
  "name": "@selectio/jobs",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bun run src/index.ts",
    "hh:login": "bun with-env bun run src/hh-login.ts",
    "hh:test": "bun with-env bun run src/test.ts",
    "hh:example": "bun run src/example-with-cookies.ts",
    "format": "biome format .",
    "lint": "biome lint .",
    "check": "biome check .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@playwright/test": "^1.56.1",
    "@t3-oss/env-core": "0.13.8",
    "crawlee": "^3.15.3",
    "playwright": "^1.56.1",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@selectio/tsconfig": "workspace:*",
    "@types/bun": "^1.3.3",
    "@types/node": "24.10.1",
    "typescript": "5.9.3"
  }
}
