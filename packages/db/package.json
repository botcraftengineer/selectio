{
  "name": "@selectio/db",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./client": {
      "default": "./src/client.ts"
    },
    "./schema": {
      "default": "./src/schema/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "drop": "bun with-env bun src/scripts/drop-schema.ts",
    "format": "biome format .",
    "lint": "biome lint .",
    "check": "biome check .",
    "push": "bun with-env drizzle-kit push",
    "studio": "bun with-env drizzle-kit studio",
    "generate-key": "bun src/scripts/generate-encryption-key.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@selectio/config": "workspace:*",
    "@vercel/postgres": "0.10.0",
    "drizzle-orm": "0.44.7",
    "drizzle-zod": "0.8.3",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@selectio/tsconfig": "workspace:*",
    "drizzle-kit": "^0.31.7",
    "typescript": "5.9.3"
  }
}
